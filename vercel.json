{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "https://to-do-phi-opal.vercel.app/" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  
   
  